{
  "compilerOptions": {
    "noEmit": true,

    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext"],

    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",

    "types": [
      "@cloudflare/vitest-pool-workers",
      "./src/worker-env.d.ts",
      "./src/worker-env.test.d.ts"
    ],

    "paths": {
      "$lib/*": ["./src/lib/*"],
      "$routes/*": ["./src/routes/*"]
    }
  }
}
