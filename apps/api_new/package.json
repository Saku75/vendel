{
  "name": "@app/api_new",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @app/api_new --parallel \"/^dev:.*/\"",
    "dev:tsup": "tsdown --watch",
    "dev:wrangler": "wrangler dev",
