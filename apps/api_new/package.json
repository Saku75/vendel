    "hono": "^4.10.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.10.2",
    "@config/eslint": "workspace:*",
    "@config/typescript": "workspace:*",
    "@types/node": "^24.9.2",
    "drizzle-kit": "^0.31.6",
    "eslint": "^9.38.0",
    "tsdown": "^0.15.11",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.45.2"
  }
