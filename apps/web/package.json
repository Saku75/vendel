{
  "name": "@vendel/web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "pnpm run build && wrangler dev",
    "deploy": "pnpm run build && wrangler deploy --minify",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "cf-typegen": "wrangler types --env-interface WorkerEnv src/worker-env.d.ts",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "wrangler types --env-interface WorkerEnv src/worker-env.d.ts"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@fontsource-variable/inter": "^5.2.5",
    "@fontsource-variable/playwrite-dk-loopet": "^5.2.5",
    "@lucide/svelte": "^0.487.0",
    "@sveltejs/adapter-cloudflare-workers": "^2.9.0",
    "@sveltejs/kit": "^2.20.5",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/vite": "^4.1.3",
    "@vendel/eslint-config": "workspace:*",
    "eslint-plugin-svelte": "^3.5.1",
    "svelte": "^5.25.10",
    "svelte-check": "^4.1.5",
    "tailwindcss": "^4.1.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.6",
    "wrangler": "^4.9.1"
  }
}
